 ! 02	1		0	0021	1	-1	-1		// EXCLAMATION MARK, DIGIT ONE, <none>, <none> (norm='!', shift='1', AltGr='-1', AltGr+shift='')
keycode 0x0a = exclam	1				
! 03	2		0	0040	2	-1	-1		// COMMERCIAL AT, DIGIT TWO, <none>, <none> (norm='@', shift='2', AltGr='-1', AltGr+shift='')
keycode 0x0b = at	2				
! 04	3		0	00a3	3	-1	-1		// POUND SIGN, DIGIT THREE, <none>, <none> (norm='£', shift='3', AltGr='-1', AltGr+shift='')
keycode 0x0c = sterling	3				
! 05	4		0	0024	4	-1	-1		// DOLLAR SIGN, DIGIT FOUR, <none>, <none> (norm='$', shift='4', AltGr='-1', AltGr+shift='')
keycode 0x0d = dollar	4				
! 06	5		0	0025	5	-1	-1		// PERCENT SIGN, DIGIT FIVE, <none>, <none> (norm='%', shift='5', AltGr='-1', AltGr+shift='')
keycode 0x0e = percent	5				
! 07	6		0	005e	6	-1	-1		// CIRCUMFLEX ACCENT, DIGIT SIX, <none>, <none> (norm='^', shift='6', AltGr='-1', AltGr+shift='')
keycode 0x0f = asciicircum	6				
! 08	7		0	0026	7	-1	-1		// AMPERSAND, DIGIT SEVEN, <none>, <none> (norm='&', shift='7', AltGr='-1', AltGr+shift='')
keycode 0x10 = ampersand	7				
! 09	8		0	002a	8	-1	-1		// ASTERISK, DIGIT EIGHT, <none>, <none> (norm='*', shift='8', AltGr='-1', AltGr+shift='')
keycode 0x11 = asterisk	8				
! 0a	9		0	0028	9	-1	-1		// LEFT PARENTHESIS, DIGIT NINE, <none>, <none> (norm='(', shift='9', AltGr='-1', AltGr+shift='')
keycode 0x12 = parenleft	9				
! 0b	0		0	0029	0	-1	-1		// RIGHT PARENTHESIS, DIGIT ZERO, <none>, <none> (norm=')', shift='0', AltGr='-1', AltGr+shift='')
keycode 0x13 = parenright	0				
! 0c	OEM_4		0	005b	007b	001b	-1		// LEFT SQUARE BRACKET, LEFT CURLY BRACKET, ESCAPE, <none> (norm='[', shift='{', AltGr='-1', AltGr+shift='')
keycode 0x14 = bracketleft	braceleft				
! 0d	OEM_6		0	005d	007d	001d	-1		// RIGHT SQUARE BRACKET, RIGHT CURLY BRACKET, INFORMATION SEPARATOR THREE, <none> (norm=']', shift='}', AltGr='-1', AltGr+shift='')
keycode 0x15 = bracketright	braceright				
